{
  "name": "obsidian-pinbox-syncer",
  "version": "1.0.0",
  "description": "Sync your Pinbox bookmarks to your vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json"
  },
  "keywords": [
    "obsidian",
    "pinbox",
    "sync",
    "bookmarks"
  ],
  "author": "gnuhpc",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.9",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/turndown": "^5.0.6",
    "turndown": "^7.2.2"
  }
}
